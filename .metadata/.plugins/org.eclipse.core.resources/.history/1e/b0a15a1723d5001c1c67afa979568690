package zoologico;

public class Zoologico {

	public static void main(String[] args) {
		Pelicano p1, p2;
		
		p1 = new Pelicano();
		p2 = new Pelicano();
		
		p1.cor = "branco";
		p2.peso = 80;
		p2.cor = "preta";
		p2.peso = "30";
		
		p1.voar();
		p2.comer();
		
		System.out.println("o pelicano p1 é " + p1.cor);
		System.out.println("o pelicano p2 é " + p2.cor);
	}

}
